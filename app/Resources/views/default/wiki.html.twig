{% set summary = '' %}
{% spaceless %}
{#Eerste zin#}
{% if 'Ongetiteld verzetsblad' != blad.titel %}
    {% set summary = summary ~ "'''#{blad.titel}''' was een verzetsblad uit de Tweede Wereldoorlog" %}
{% endif %}
{% if blad.tijdstipEersteUitgave is defined and blad.tijdstipLaatseUitgave is defined %}
    {% set summary = summary ~ ", dat vanaf #{blad.tijdstipEersteUitgave} tot en met ${blad.tijdstipLaatseUitgave} " %}
{% endif %}
{% if blad.tijdstipEersteUitgave is defined and blad.tijdstiplaatsetUitgave is not defined %}
    {%  set summary = summary ~ ",dat vanaf ${blad.tijdstipEersteUitgave} " %}
{% endif %}
{% if blad.tijdstipEersteUitgave is not defined and blad.tijdstipLaatseUitgave is defined %}
    {% set summary = summary ~ ", dat tot en met #{blad.tijdstipLaatseUitgave} " %}
{% endif %}
{% if blad.PlaatsUitgave is defined %}
    {% if blad.tijdstipEersteUitgave is not defined and blad.tijdstipLaatseUitgave is not defined %}
        {% set summary = summary ~ 'dat ' %}
    {% endif %}
    {% set summary = summary ~ "in #{blad.PlaatsUitgave} " %}
{% endif %}
{% if blad.tijdstipEersteUitgave is defined or blad.tijdstipLaatseUitgave is defined or blad.PlaatsUitgave is defined %}
    {% set summary = summary ~ 'werd uitgegeven' %}
{% endif %}
{% set summary = summary ~ '.' %}
{#Tweede zin#}
{% if blad.drukkerij is defined %}
    {% set summary = summary ~ " Het blad werd gemaakt bij drukkerij #{blad.drukkerij}" %}
    {% if blad.frequentie is defined or blad.oplageMin is defined or blad.oplageMax is defined %}
        {% set summary = summary ~ ' en verscheen ' %}
    {% else %}
        {% set summary = summary ~ '. ' %}
    {% endif %}
{% else %}
    {% if blad.frequentie is defined or blad.oplageMin is defined or blad.oplageMax is defined %}
        {% set summary = summary ~ ' Het blad verscheen' %}
    {% endif %}
    {% if blad.frequentie is defined %}
        {% set summary = summary ~ " #{blad.frequentie}" %}
    {% endif %}
    {% if blad.oplageMin is defined or blad.oplageMax is defined %}
        {% set summary = summary ~ ' in een oplage' %}
        {% if blad.oplageMin < blad.oplageMax %}
            {% set summary = summary ~ " tussen de #{blad.oplageMin} en #{blad.oplageMax} exemplaren" %}
        {% endif %}
        {% if blad.oplageMin == blad.oplageMax %}
            {% set summary = summary ~ " van #{blad.oplageMin} exemplaren" %}
        {% endif %}
    {% endif %}
    {% set summary = summary ~ "." %}
{% endif %}
{#Derde zin#}
{% if blad.reprMeth is defined and blad.inhoud is defined %}
    {% set summary = summary ~ " Het werd #{blad.reprMeth} en de inhoud bestond voornamelijk uit #{blad.inhoud|preg_replace('/([A-Z])/', ' $1')|lower|trim}." %}
{% endif %}
{% if blad.reprMeth is defined and blad.inhoud is not defined %}
    {% set summary = summary ~ " Het werd #{blad.reprMeth}." %}
{% endif %}
{% if blad.reprMeth is not defined and blad.inhoud is defined %}
    {% set summary = summary ~ " De inhoud bestond voornamelijk uit #{blad.inhoud}." %}
{% endif %}
{% endspaceless %}
