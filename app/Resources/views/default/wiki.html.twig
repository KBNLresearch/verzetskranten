| drukkers                = {% for drukker in drukkers %}[[{{ drukker.naam }}]] {% endfor %} 
| andere betrokkenen      = 
| gerelateerd             = {% for relatie in relaties %}[[{{ relatie.EindWiki|default('Ongetiteld verzetsblad') }}|{{ relatie.Eindblad|default('Ongetiteld verzetsblad') }}]] {% endfor %} 
| opvolger van            = 
| voorloper van           = 
| legaal voortgezet       = 
